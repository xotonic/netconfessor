<config xmlns="http://eltex.org/ap" xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
  <config-description>
    <version>wop-2ac-lr5 1.0</version>
  </config-description>
  <interface>
    <name>eth0</name>
    <common>
      <type>eth</type>
      <enabled>1</enabled>
      <wan>0</wan>
      <dns-server-1></dns-server-1>
      <dns-server-2></dns-server-2>
      <dhcp-server/>
      <access-rules>
        <telnet>1</telnet>
        <ssh>1</ssh>
        <snmp>1</snmp>
        <netconf>1</netconf>
        <web>1</web>
      </access-rules>
    </common>
    <eth/>
  </interface>
  <interface>
    <name>wlan1</name>
    <common>
      <type>wlan</type>
      <enabled>1</enabled>
      <wan>0</wan>
      <dns-server-1></dns-server-1>
      <dns-server-2></dns-server-2>
      <access-rules>
        <telnet>1</telnet>
        <ssh>1</ssh>
        <snmp>1</snmp>
        <netconf>1</netconf>
        <web>1</web>
      </access-rules>
    </common>
    <wlan>
      <mode>radio</mode>
      <rssi-indication>1</rssi-indication>
      <radio>
        <country>RU</country>
        <work-mode>ac</work-mode>
        <bandwidth>80</bandwidth>
        <disable-atf>0</disable-atf>
        <tdma>0</tdma>
        <tdma-tx-duration>10000</tdma-tx-duration>
        <atf-iftime>1000</atf-iftime>
        <control-sideband>upper</control-sideband>
        <tx-power>24</tx-power>
        <wmm>1</wmm>
        <vap-enable>1</vap-enable>
        <fragmentation-threshold>2346</fragmentation-threshold>
        <rts-threshold>2347</rts-threshold>
        <short-preamble>1</short-preamble>
        <aggregation>1</aggregation>
        <sgi>1</sgi>
        <stbc>0</stbc>
        <obss-coex>0</obss-coex>
        <beamforming>0</beamforming>
        <ack-timeout>64</ack-timeout>
        <protection-disabled>1</protection-disabled>
        <channel>36</channel>
        <enable-fixed-rate>0</enable-fixed-rate>
        <fixed-rate></fixed-rate>
        <tx-broadcast-limit>0</tx-broadcast-limit>
        <dfs>forced</dfs>
        <fbwa-mode>0</fbwa-mode>
        <beacon-interval>100</beacon-interval>
        <qos>
          <enable>0</enable>
          <edca-ap>
            <bk>
              <acm>1</acm>
              <aifs>7</aifs>
              <cwmin>15</cwmin>
              <cwmax>1023</cwmax>
              <txop>0</txop>
            </bk>
            <be>
              <acm>1</acm>
              <aifs>3</aifs>
              <cwmin>15</cwmin>
              <cwmax>63</cwmax>
              <txop>0</txop>
            </be>
            <vi>
              <acm>1</acm>
              <aifs>1</aifs>
              <cwmin>7</cwmin>
              <cwmax>15</cwmax>
              <txop>94</txop>
            </vi>
            <vo>
              <acm>1</acm>
              <aifs>1</aifs>
              <cwmin>3</cwmin>
              <cwmax>7</cwmax>
              <txop>47</txop>
            </vo>
          </edca-ap>
          <edca-sta>
            <bk>
              <acm>1</acm>
              <aifs>7</aifs>
              <cwmin>15</cwmin>
              <cwmax>1023</cwmax>
              <txop>0</txop>
            </bk>
            <be>
              <acm>1</acm>
              <aifs>3</aifs>
              <cwmin>15</cwmin>
              <cwmax>1023</cwmax>
              <txop>0</txop>
            </be>
            <vi>
              <acm>1</acm>
              <aifs>2</aifs>
              <cwmin>7</cwmin>
              <cwmax>15</cwmax>
              <txop>94</txop>
            </vi>
            <vo>
              <acm>1</acm>
              <aifs>2</aifs>
              <cwmin>3</cwmin>
              <cwmax>7</cwmax>
              <txop>47</txop>
            </vo>
          </edca-sta>
        </qos>
      </radio>
      <sta>
        <security-mode>off</security-mode>
        <client-shaper-mode>off</client-shaper-mode>
        <shaper-limit>0</shaper-limit>
        <key-wep></key-wep>
        <key-wpa></key-wpa>
        <ssid>WOP-2ac-LR5</ssid>
        <vlan-support>0</vlan-support>
        <radius>
          <username>user</username>
          <password>password</password>
        </radius>
      </sta>
      <ap>
        <station-isolation>0</station-isolation>
        <inactivity-timeout>120</inactivity-timeout>
        <shaper-per-vap-tx>
          <mode>off</mode>
          <value>0</value>
        </shaper-per-vap-tx>
        <shaper-per-vap-rx>
          <mode>off</mode>
          <value>0</value>
        </shaper-per-vap-rx>
        <shaper-per-sta-tx>
          <mode>off</mode>
          <value>0</value>
        </shaper-per-sta-tx>
        <shaper-per-sta-rx>
          <mode>off</mode>
          <value>0</value>
        </shaper-per-sta-rx>
        <vlan-id>0</vlan-id>
        <vlan-priority>auto</vlan-priority>
        <vlan-trunk>0</vlan-trunk>
        <vlan-support>1</vlan-support>
        <priority-by-dscp>1</priority-by-dscp>
        <security-mode>off</security-mode>
        <hidden>0</hidden>
        <sta-limit>0</sta-limit>
        <iapp>1</iapp>
        <key-wep></key-wep>
        <key-wpa></key-wpa>
        <ssid></ssid>
        <radius>
          <auth-port>1812</auth-port>
          <auth-address>192.168.0.1</auth-address>
          <domain>root</domain>
          <auth-password>secret</auth-password>
          <acct-enable>0</acct-enable>
          <acct-port>1814</acct-port>
          <acct-periodic>0</acct-periodic>
          <acct-interval>30</acct-interval>
          <acct-address>192.168.0.1</acct-address>
          <acct-password>secret</acct-password>
        </radius>
      </ap>
    </wlan>
  </interface>
  <interface>
    <name>wlan1-va0</name>
    <common>
      <type>vap</type>
      <enabled>1</enabled>
      <wan>0</wan>
      <dns-server-1></dns-server-1>
      <dns-server-2></dns-server-2>
      <dhcp-server>
        <static-lease>
          <name>11:11:11:11:11:11</name>
          <ip-address>192.168.26.228</ip-address>
        </static-lease>
      </dhcp-server>
      <access-rules>
        <telnet>1</telnet>
        <ssh>1</ssh>
        <snmp>1</snmp>
        <netconf>1</netconf>
        <web>1</web>
      </access-rules>
    </common>
    <vap>
      <basic>wlan1</basic>
      <sta-limit>0</sta-limit>
      <vap-id>0</vap-id>
      <acl>
        <mode>blacklist</mode>
      </acl>
      <inactivity-timeout>120</inactivity-timeout>
      <shaper-per-vap-tx>
        <mode>kbps</mode>
        <value>4096</value>
      </shaper-per-vap-tx>
      <shaper-per-vap-rx>
        <mode>kbps</mode>
        <value>4096</value>
      </shaper-per-vap-rx>
      <shaper-per-sta-tx>
        <mode>kbps</mode>
        <value>4096</value>
      </shaper-per-sta-tx>
      <shaper-per-sta-rx>
        <mode>kbps</mode>
        <value>4096</value>
      </shaper-per-sta-rx>
      <hidden>0</hidden>
      <station-isolation>0</station-isolation>
      <vlan-id>0</vlan-id>
      <vlan-priority>auto</vlan-priority>
      <vlan-trunk>0</vlan-trunk>
      <vlan-support>1</vlan-support>
      <priority-by-dscp>1</priority-by-dscp>
      <security-mode>WPA</security-mode>
      <key-wep></key-wep>
      <key-wpa>password</key-wpa>
      <ssid>WOP-2ac-LR5_EMS</ssid>
      <radius>
        <auth-port>1812</auth-port>
        <auth-address>192.168.0.1</auth-address>
        <auth-password>secret</auth-password>
        <acct-enable>0</acct-enable>
        <acct-port>1813</acct-port>
        <acct-address>192.168.0.1</acct-address>
        <acct-password>secret</acct-password>
        <acct-periodic>0</acct-periodic>
        <acct-interval>30</acct-interval>
        <domain>root</domain>
      </radius>
    </vap>
  </interface>
  <interface>
    <name>wlan1-va1</name>
    <common>
      <type>vap</type>
      <enabled>0</enabled>
      <wan>0</wan>
      <dns-server-1></dns-server-1>
      <dns-server-2></dns-server-2>
      <access-rules>
        <telnet>1</telnet>
        <ssh>1</ssh>
        <snmp>1</snmp>
        <netconf>1</netconf>
        <web>0</web>
      </access-rules>
    </common>
    <vap>
      <basic>wlan1</basic>
      <sta-limit>0</sta-limit>
      <vap-id>1</vap-id>
      <acl>
        <mode>blacklist</mode>
      </acl>
      <inactivity-timeout>120</inactivity-timeout>
      <shaper-per-vap-tx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-vap-tx>
      <shaper-per-vap-rx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-vap-rx>
      <shaper-per-sta-tx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-sta-tx>
      <shaper-per-sta-rx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-sta-rx>
      <hidden>0</hidden>
      <station-isolation>0</station-isolation>
      <vlan-id>0</vlan-id>
      <vlan-priority>auto</vlan-priority>
      <vlan-trunk>0</vlan-trunk>
      <vlan-support>1</vlan-support>
      <priority-by-dscp>1</priority-by-dscp>
      <security-mode>off</security-mode>
      <key-wep></key-wep>
      <key-wpa></key-wpa>
      <ssid>WOP-2ac-LR5-1</ssid>
      <radius>
        <auth-port>1812</auth-port>
        <auth-address>192.168.0.1</auth-address>
        <auth-password>secret</auth-password>
        <acct-enable>0</acct-enable>
        <acct-port>1813</acct-port>
        <acct-address>192.168.0.1</acct-address>
        <acct-password>secret</acct-password>
        <acct-periodic>0</acct-periodic>
        <acct-interval>30</acct-interval>
        <domain>root</domain>
      </radius>
    </vap>
  </interface>
  <interface>
    <name>wlan1-va2</name>
    <common>
      <type>vap</type>
      <enabled>0</enabled>
      <wan>0</wan>
      <dns-server-1></dns-server-1>
      <dns-server-2></dns-server-2>
      <access-rules>
        <telnet>1</telnet>
        <ssh>1</ssh>
        <snmp>1</snmp>
        <netconf>1</netconf>
        <web>1</web>
      </access-rules>
    </common>
    <vap>
      <basic>wlan1</basic>
      <sta-limit>0</sta-limit>
      <vap-id>2</vap-id>
      <acl>
        <mode>blacklist</mode>
        <mac>
          <name>aa:aa:aa:aa:aa:aa</name>
        </mac>
        <mac>
          <name>bb:bb:bb:bb:bb:bb</name>
        </mac>
      </acl>
      <inactivity-timeout>120</inactivity-timeout>
      <shaper-per-vap-tx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-vap-tx>
      <shaper-per-vap-rx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-vap-rx>
      <shaper-per-sta-tx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-sta-tx>
      <shaper-per-sta-rx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-sta-rx>
      <hidden>0</hidden>
      <station-isolation>0</station-isolation>
      <vlan-id>0</vlan-id>
      <vlan-priority>auto</vlan-priority>
      <vlan-trunk>0</vlan-trunk>
      <vlan-support>1</vlan-support>
      <priority-by-dscp>1</priority-by-dscp>
      <security-mode>off</security-mode>
      <key-wep></key-wep>
      <key-wpa></key-wpa>
      <ssid>WOP-2ac-LR5-2</ssid>
      <radius>
        <auth-port>1812</auth-port>
        <auth-address>192.168.0.1</auth-address>
        <auth-password>secret</auth-password>
        <acct-enable>0</acct-enable>
        <acct-port>1813</acct-port>
        <acct-address>192.168.0.1</acct-address>
        <acct-password>secret</acct-password>
        <acct-periodic>0</acct-periodic>
        <acct-interval>30</acct-interval>
        <domain>root</domain>
      </radius>
    </vap>
  </interface>
  <interface>
    <name>wlan1-va3</name>
    <common>
      <type>vap</type>
      <enabled>0</enabled>
      <wan>0</wan>
      <dns-server-1></dns-server-1>
      <dns-server-2></dns-server-2>
      <access-rules>
        <telnet>1</telnet>
        <ssh>1</ssh>
        <snmp>1</snmp>
        <netconf>1</netconf>
        <web>1</web>
      </access-rules>
    </common>
    <vap>
      <basic>wlan1</basic>
      <sta-limit>0</sta-limit>
      <vap-id>3</vap-id>
      <inactivity-timeout>120</inactivity-timeout>
      <shaper-per-vap-tx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-vap-tx>
      <shaper-per-vap-rx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-vap-rx>
      <shaper-per-sta-tx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-sta-tx>
      <shaper-per-sta-rx>
        <mode>off</mode>
        <value>0</value>
      </shaper-per-sta-rx>
      <hidden>0</hidden>
      <station-isolation>0</station-isolation>
      <vlan-id>0</vlan-id>
      <vlan-priority>auto</vlan-priority>
      <vlan-trunk>0</vlan-trunk>
      <vlan-support>1</vlan-support>
      <priority-by-dscp>1</priority-by-dscp>
      <security-mode>off</security-mode>
      <key-wep></key-wep>
      <key-wpa></key-wpa>
      <ssid>WOP-2ac-LR5-3</ssid>
      <radius>
        <auth-port>1812</auth-port>
        <auth-address>192.168.0.1</auth-address>
        <auth-password>secret</auth-password>
        <acct-enable>0</acct-enable>
        <acct-port>1813</acct-port>
        <acct-address>192.168.0.1</acct-address>
        <acct-password>secret</acct-password>
        <acct-periodic>0</acct-periodic>
        <acct-interval>30</acct-interval>
        <domain>root</domain>
      </radius>
    </vap>
  </interface>
  <interface>
    <name>br0</name>
    <common>
      <type>bridge</type>
      <enabled>1</enabled>
      <wan>1</wan>
      <protocol>dhcp</protocol>
      <disable-broadcast-dhcp>0</disable-broadcast-dhcp>
      <static-ip>192.168.1.10</static-ip>
      <netmask>255.255.255.0</netmask>
      <dns-server-1>172.16.0.250</dns-server-1>
      <dns-server-2>172.16.0.100</dns-server-2>
      <access-rules>
        <telnet>1</telnet>
        <ssh>1</ssh>
        <snmp>1</snmp>
        <netconf>1</netconf>
        <web>1</web>
        <web-https>1</web-https>
      </access-rules>
    </common>
    <bridge>
      <port>
        <name>eth0</name>
      </port>
      <port>
        <name>wlan1-va0</name>
      </port>
      <port>
        <name>wlan1-va1</name>
      </port>
      <port>
        <name>wlan1-va2</name>
      </port>
    </bridge>
  </interface>
  <web-configurator>
    <enable>1</enable>
    <http-port>80</http-port>
    <https-port>443</https-port>
  </web-configurator>
  <authentication>
    <admin-password>password</admin-password>
  </authentication>
  <snmp>
    <enable>1</enable>
    <rocommunity>public</rocommunity>
    <rwcommunity>privatee</rwcommunity>
    <trapsink></trapsink>
    <trap2sink></trap2sink>
    <informsink></informsink>
    <sysname>WOP-2ac-LR5</sysname>
    <syscontact>Contact</syscontact>
    <syslocation>Russia</syslocation>
    <trapcommunity>trap</trapcommunity>
  </snmp>
  <netconf>
    <enable>1</enable>
  </netconf>
  <date-time>
    <mode>ntp</mode>
    <common>
      <timezone>Europe/Moscow (Moscow,Russia)</timezone>
      <dstenable>0</dstenable>
      <dststart></dststart>
      <dstend></dstend>
      <dstoffset>60</dstoffset>
    </common>
    <manual/>
    <ntp>
      <server>pool.ntp.org</server>
    </ntp>
  </date-time>
  <trace>
    <networkd>
      <error>1</error>
      <warn>0</warn>
      <info>0</info>
      <debug>0</debug>
      <traffic>0</traffic>
    </networkd>
  </trace>
  <system>
    <global-station-isolation>0</global-station-isolation>
  </system>
</config>
